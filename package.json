{
	"name": "mr-velous",
	"description": "MR Viewer with Enhanced Layout and Optimised User Satisfaction",
	"version": "0.5.0",
	"private": true,
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igoresso/mr-velous/issues"
	},
	"homepage": "https://igoresso.github.io/mr-velous/",
	"scripts": {
		"copy:nifti": "cpx \"node_modules/@itk-wasm/image-io/dist/pipelines/nifti-read-image.{js,wasm.zst}\" static/pipelines",
		"copy:dicom-series": "cpx \"node_modules/@itk-wasm/dicom/dist/pipelines/read-image-dicom-file-series.{js,wasm.zst}\" static/pipelines",
		"copy:dicom-tags": "cpx \"node_modules/@itk-wasm/dicom/dist/pipelines/read-dicom-tags.{js,wasm.zst}\" static/pipelines",
		"clean:pipelines": "rimraf static/pipelines",
		"pipelines": "run-s clean:pipelines copy:nifti copy:dicom-series copy:dicom-tags",
		"dev": "vite dev",
		"prebuild": "npm run pipelines",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@itk-wasm/dicom": "^7.2.3",
		"@itk-wasm/image-io": "^1.3.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-selection": "^3.0.11",
		"@types/d3-zoom": "^3.0.8",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.34",
		"clsx": "^2.1.1",
		"cpx": "^1.5.0",
		"d3-scale": "^4.0.2",
		"d3-selection": "^3.0.0",
		"d3-zoom": "^3.0.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"itk-wasm": "1.0.0-b.179",
		"jsdom": "^25.0.1",
		"lucide-svelte": "^0.454.0",
		"mode-watcher": "^0.4.1",
		"npm-run-all": "^4.1.5",
		"paneforge": "1.0.0-next.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"rimraf": "^6.0.1",
		"svelte": "^5.1.6",
		"svelte-check": "^4.0.5",
		"svelte-radix": "^2.0.1",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"vite": "^5.4.10",
		"vite-plugin-static-copy": "^2.0.0",
		"vitest": "^2.1.4"
	},
	"type": "module"
}
