{
	"name": "mr-velous",
	"description": "MR Viewer with Enhanced Layout and Optimised User Satisfaction",
	"version": "0.5.0",
	"private": true,
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igoresso/mr-velous/issues"
	},
	"homepage": "https://igoresso.github.io/mr-velous/",
	"scripts": {
		"copy:nifti": "cpx \"node_modules/@itk-wasm/image-io/dist/pipelines/nifti-read-image.{js,wasm.zst}\" static/pipelines",
		"copy:dicom-series": "cpx \"node_modules/@itk-wasm/dicom/dist/pipelines/read-image-dicom-file-series.{js,wasm.zst}\" static/pipelines",
		"copy:dicom-tags": "cpx \"node_modules/@itk-wasm/dicom/dist/pipelines/read-dicom-tags.{js,wasm.zst}\" static/pipelines",
		"pipelines": "run-s copy:nifti copy:dicom-series copy:dicom-tags",
		"dev": "vite dev",
		"prebuild": "pnpm run pipelines",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.0",
		"@eslint/js": "^9.29.0",
		"@itk-wasm/dicom": "^7.6.0",
		"@itk-wasm/image-io": "^1.6.0",
		"@lucide/svelte": "^0.518.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/postcss": "^4.1.10",
		"@tailwindcss/vite": "^4.1.10",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-selection": "^3.0.11",
		"@types/d3-zoom": "^3.0.8",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"bits-ui": "2.8.3",
		"clsx": "^2.1.1",
		"cpx": "^1.5.0",
		"d3-scale": "^4.0.2",
		"d3-selection": "^3.0.0",
		"d3-zoom": "^3.0.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.3",
		"globals": "^16.2.0",
		"itk-wasm": "1.0.0-b.188",
		"jsdom": "^26.1.0",
		"mode-watcher": "^1.0.8",
		"npm-run-all": "^4.1.5",
		"paneforge": "1.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"svelte": "^5.34.7",
		"svelte-check": "^4.2.2",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.10",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"vite": "^6.3.5",
		"vitest": "^3.2.4"
	},
	"type": "module"
}
